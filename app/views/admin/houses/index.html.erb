<div class="container pt-3 pb-3">
  <div class="mb-3">
  <a href="new"><button type="button" class="btn btn-primary">新規登録</button></a>
  </div>
  
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">name</th>
      <th scope="col">address</th>
      <th scope="col">access</th>
      <th scope="col">description</th>
      <th scope="col">gender_type</th>
      <th scope="col">物件の編集</th>
    </tr>
  </thead>
  <tbody>
    <% @houses.each do |house| %>
    <tr>
      <th scope="row"><%= house.id %></th>
      <td><%= house.name %></td>
      <td><%= house.address %></td>
      <td><%= house.access %></td>
      <td><%= house.description %></td>
      <td><%= house.gender_type %></td>
      <td><a href="new"><button type="button" class="btn btn-primary">編集</button></a>
  </div></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>